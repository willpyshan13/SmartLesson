<?xml version="1.0" encoding="UTF-8"?><!--<layer-list xmlns:android="http://schemas.android.com/apk/res/android">--><!--&lt;!&ndash; 背景图 &ndash;&gt;--><!--<item android:id="@android:id/background" android:drawable="@drawable/bfq_jdt_sc" />--><!--&lt;!&ndash; 第二进度图 &ndash;&gt;--><!--<item android:id="@android:id/secondaryProgress" android:drawable="@drawable/bfq_jdt_sc" />--><!--&lt;!&ndash; 进度 &ndash;&gt;--><!--<item android:id="@android:id/progress" android:drawable="@drawable/bfq_jdt_yk" />--><!--</layer-list>-->

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background"
        android:height="3dp">
        <shape>
            <size android:height="0.1dp"></size>
            <solid android:color="#8CC63F" />
            <corners android:radius="25dp"></corners>
        </shape>
    </item>
    <item
        android:id="@android:id/progress"
        android:height="3dp">
        <clip>
            <shape>
                <size android:height="0.5dp"></size>
                <solid android:color="#006837" />
                <corners android:radius="25dp"></corners>
            </shape>
        </clip>
    </item>
</layer-list>